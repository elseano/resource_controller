<h1>New <%= resource_controller_options[:object_name] || model_name %></h1>

<%= error_messages_for model_name.to_sym %>

<% form_for(model_name.to_sym, :url => collection_url) do |f| %>
<%= render "form", :form => f %>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', collection_url %>